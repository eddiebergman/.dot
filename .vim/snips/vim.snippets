snippet section "{{{ .. }}}"
" {{{ ${1:Short Desc.}
" }}}
${0}
endsnippet

snippet nnoremap " nnorremap: description .. <..>"
" normal: ${1:Description}
nnoremap ${0}
endsnippet

snippet inoremap " inorremap: description .. <..>"
" insert: ${1:Description}
inoremap ${0}
endsnippet

snippet vnoremap " vnorremap: description .. <..>"
" visual: ${1:Description}
vnoremap ${0}
endsnippet

snippet cnoremap " cnorremap: description .. <..>"
" command: ${1:Description}
cnoremap ${0}
endsnippet
